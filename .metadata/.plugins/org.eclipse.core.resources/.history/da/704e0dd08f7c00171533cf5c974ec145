#ifndef NODE_H_
#define NODE_H_
#include <iostream>
using namespace std;

template <class T>
class Node{
private:
	T data;
	Node<T> *next;
public:
	Node();
	Node(T val);
	Node(const Node<T>& newNode);
	Node& operator=(const Node<T>& otherNode);
};



#endif /* NODE_H_ */
