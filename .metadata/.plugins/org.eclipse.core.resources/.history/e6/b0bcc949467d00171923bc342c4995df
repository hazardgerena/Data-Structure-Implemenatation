#ifndef NODE_H_
#define NODE_H_
#include <iostream>
using namespace std;

template <class T>
class Node{
public:
	T data;
	Node<T> *next;
	Node(void);
	Node(T);
	Node(const Node<T>& );
	Node& operator=(const Node<T>& );
	~Node(void);
};

#include "Node.cpp"

#endif /* NODE_H_ */
