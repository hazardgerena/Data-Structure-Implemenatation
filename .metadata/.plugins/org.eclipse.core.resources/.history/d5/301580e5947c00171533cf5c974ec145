#include "Node.h"
template <class T>
Node<T>::Node():data(0),next(){};

template <class T>
Node<T>::Node(T val){
	this->data = val;
	this->next = NULL;
}

template <class T>
Node<T>::Node(const Node<T>& newNode){
	this->data = newNode.data;
	this->next = newNode.next;
}

template <class T>
Node<T>& Node<T>::operator =(const Node<T>& otherNode){
	this->data = otherNode.data;
	this->next = otherNode.next;
}
