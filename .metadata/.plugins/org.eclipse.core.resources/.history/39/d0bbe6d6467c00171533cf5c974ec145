#ifndef LIST_
#define LIST_

#include <iostream>
using namespace std;

template<class T>
struct Node{
	T data;
	Node* next;
	Node(T val){
		this->data = val;
	}

};

template <class T>
class LinkedList{
private:
	Node<T> *head;
	Node<T> *tail;
public:
	LinkedList();
	LinkedList(Node<T>* newNode);
	LinkedList(const LinkedList& obj);
	void addTail(Node<T> val);
	void addHead(Node<T> val);
	LinkedList& operator=(const LinkedList List);
	~LinkedList();


};


#endif /* LIST_ */
